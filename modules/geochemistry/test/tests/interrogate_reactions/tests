[Tests]
  [./invalid_swap]
    type = RunException
    input = invalid_swap.i
    expect_err = 'Matrix is not invertible, which signals an invalid basis swap'
    requirement = 'The geochemistry interrogator will throw an error for an invalid swap'
    issues = '#14981'
    design = 'GeochemicalModelInterrogator.md GeochemicalModelRoot.md'
  [../]
  [./gypsum]
    type = 'RunApp'
    input = 'gypsum.i'
    expect_out = 'Not enough activites known to compute equilibrium temperature for reaction
  Gypsum = 2\*H2O \+ 1\*Ca\+\+ \+ 1\*SO4--  .  log10\(K\) = -4.451
Gypsum.  T = 43.6625degC'
    requirement = "The geochemistry interrogator shall be able to compute temperature at equilibrium, given enough information about activity values"
    issues = '#14981'
    design = 'eqm_temp_a.md GeochemicalModelInterrogator.md GeochemicalModelRoot.md'
  [../]
  [./hematite]
    type = 'RunApp'
    input = 'hematite.i'
    expect_out = '\(A_H\+\)\^-4 \(A_O2\(aq\)\)\^0.5 = 10\^3.066'
    requirement = "The geochemistry interrogator shall be output equations involving pH and pe for equilibrium reactions"
    issues = '#14981'
    design = 'pH_pe.md GeochemicalModelInterrogator.md GeochemicalModelRoot.md'
  [../]
  [./muscovite]
    type = 'RunApp'
    input = 'muscovite.i'
    expect_out = '\(A_K\+\)\^1 \(A_Al\+\+\+\)\^3 \(A_SiO2\(aq\)\)\^3 \(A_H\+\)\^-10 = 10\^14.57
\(A_K\+\)\^1 \(A_H\+\)\^-1 = 10\^3.419'
    requirement = "The geochemistry interrogator shall output activity ratios for different choice of basis components."
    issues = '#14981'
    design = 'activity_ratios.md GeochemicalModelInterrogator.md GeochemicalModelRoot.md'
  [../]
  [./muscovite2]
    type = 'RunApp'
    input = 'muscovite2.i'
    expect_out = '\(A_H2O\)\^6 \(A_K\+\)\^1 \(A_Al\+\+\+\)\^3 \(A_SiO2\(aq\)\)\^3 \(A_H\+\)\^-10 = 10\^14.57
\(A_H2O\)\^6 \(A_K\+\)\^1 \(A_Al\+\+\+\)\^3 \(A_H\+\)\^-10 = 10\^26.6
\(A_K\+\)\^-2 \(A_H\+\)\^2 = 10\^-9.682
\(A_K\+\)\^-2 \(A_SiO2\(aq\)\)\^-6 \(A_H\+\)\^2 = 10\^14.38
\(A_K\+\)\^-2 \(A_H\+\)\^2 = 10\^-8.687
\(A_K\+\)\^-2 \(A_SiO2\(aq\)\)\^-6 \(A_H\+\)\^2 = 10\^14.38
\(A_K\+\)\^-2 \(A_H\+\)\^2 = 10\^-1.947'
    requirement = "The geochemistry interrogator shall output activity ratios for different choice of basis components."
    issues = '#14981'
    design = 'activity_ratios.md GeochemicalModelInterrogator.md GeochemicalModelRoot.md'
  [../]
  [./pyrite]
    type = 'RunApp'
    input = 'pyrite.i'
    expect_out = 'Pyrite = -1\*H2O \+ 1\*Fe\+\+ \+ 2\*SO4-- \+ 2\*H\+ - 3.5\*O2\(aq\)  .  log10\(K\) = 217.4
Pyrite = -3.5\*H2O \+ 1\*Fe\(OH\)3\(ppd\) \+ 2\*SO4-- \+ 4\*H\+ - 3.75\*O2\(aq\)  .  log10\(K\) = 221
Pyrite = -1\*H2O \+ 1\*Fe\+\+ \+ 2\*SO4-- \+ 2\*H\+ - 3.5\*O2\(aq\)  .  log10\(K\) = 217.4
Pyrite = -1\*H2O \+ 1\*Fe\+\+ \+ 2\*H2S\(aq\) - 2\*H\+ \+ 0.5\*O2\(aq\)  .  log10\(K\) = -45.24
Pyrite = -1\*H2O \+ 1\*Fe\+\+ \+ 1.75\*H2S\(aq\) - 1.5\*H\+ \+ 0.25\*SO4--  .  log10\(K\) = -12.41'
    requirement = "The geochemistry interrogator shall output reactions and equilibrium constants for different choice of basis components."
    issues = '#14981'
    design = 'reaction_balancing.md GeochemicalModelInterrogator.md GeochemicalModelRoot.md'
  [../]
  [./clinoptilolite]
    type = 'RunApp'
    input = 'clinoptilolite.i'
